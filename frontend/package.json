{
  "name": "edge-ai-sizing-tool",
  "version": "2025.03.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "",
  "type": "module",
  "scripts": {
    "setup:workers": "node scripts/setup-workers.mjs",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dist": "npm install && STANDALONE_BUILD=true npm run build && node scripts/dist.mjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack -p 8080",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 8080 --hostname 127.0.0.1",
    "demo": "node scripts/run.mjs",
    "stop": "pm2 save && pm2 stop all"
  },
  "dependencies": {
    "@payloadcms/db-sqlite": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/postcss": "^4.0.12",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "image-size": "^2.0.2",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.479.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.6",
    "openvino-node": "^2025.0.0",
    "payload": "latest",
    "pm2": "^6.0.8",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "recharts": "^2.15.1",
    "sharp": "^0.34.1",
    "sonner": "^2.0.1",
    "swr": "^2.3.3",
    "systeminformation": "^5.26.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.12",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "archiver": "^7.0.1",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.2.0",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "webpack": "^5.98.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
